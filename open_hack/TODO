  case __NR_statfs:             /* fall through */
  case __NR_link:               /* fall through */
  case __NR_unlink:             /* fall through */
  case __NR_chmod:              /* fall through */
  case __NR_chown:              /* fall through */
  case __NR_lchown:             /* fall through */
  case __NR_mkdir:              /* fall through */
  case __NR_rmdir:              /* fall through */
